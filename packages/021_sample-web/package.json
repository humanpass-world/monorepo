{
  "name": "@hp/sample-web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "yarn typegen && vite --port 8284 --host",
    "build": "tsc -b || true && vite build",
    "lint": "eslint .",
    "preview": "npm run build && vite preview",
    "deploy:production": "tsc -b || true && CLOUDFLARE_ENV=production vite build --mode production && wrangler deploy",
    "typegen": "wrangler types -c wrangler.jsonc"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "@rainbow-me/rainbowkit": "^2.2.8",
    "@tanstack/react-query": "^5.90.2",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.544.0",
    "postcss": "^8.5.6",
    "react": "^19.1.1",
    "react-confetti": "^6.4.0",
    "react-dom": "^19.1.1",
    "shadcn-ui": "^0.9.5",
    "tailwind-merge": "^3.3.1",
    "viem": "^2.37.9",
    "wagmi": "^2.17.5"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.13.7",
    "@eslint/js": "^9.33.0",
    "@tailwindcss/vite": "^4.1.13",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "tailwindcss": "3",
    "tw-animate-css": "^1.2.9",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2",
    "wrangler": "4.40.0"
  }
}
